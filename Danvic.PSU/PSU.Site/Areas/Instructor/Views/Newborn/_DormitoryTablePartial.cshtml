<div class="col-xs-12">
    <div class="box">
        <div class="box-body">
            <table id="bulletin-table" class="table table-bordered table-striped">
                <thead>
                    <tr>
                        <th class="text-center">学生学号</th>
                        <th class="text-center">学生姓名</th>
                        <th class="text-center">所属班级</th>
                        <th class="text-center">床铺号</th>
                        <th class="text-center">寝室号</th>
                        <th class="text-center">所在楼层</th>
                        <th class="text-center">寝室楼</th>
                        <th class="text-center">选择时间</th>
                    </tr>
                </thead>
                <tbody>
                    @for (int i = 0; i < 15; i++)
                    {
                        <tr class="text-center">
                            @{
                                int num = 20171011 + i;
                            }
                            <td>@num</td>
                            <td>张三</td>
                            <td>网络1401</td>
                            <td>A18-S212-01</td>
                            <td>A18-S212</td>
                            <td>二层</td>
                            <td>A18</td>
                            <td>@DateTime.Now.ToString()</td>
                        </tr>
                    }
                </tbody>
            </table>
        </div>
    </div>
</div>
